generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

// Define table structure
model Account {
  id        Int       @id @default(autoincrement())
  name      String
  email     String    @unique
  gender    GENDER
  age       Int?
  createdAt DateTime  @default(now())
  updatedAt DateTime? @updatedAt
}

enum GENDER {
  MALE
  FEMALE
}
